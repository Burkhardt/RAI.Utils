<Project Sdk="Microsoft.NET.Sdk">

<PropertyGroup>
    <Title>RaiUtils</Title>
    <PackageId>RaiUtils</PackageId>
    <Version>2.1.0</Version>
    <Authors>Rainer Burkhardt</Authors>
    <Description>Random, Email, ParameterDictionary</Description>
    <!--<PackageDescription>Random, Email, ParameterDictionary</PackageDescription>-->
    <TargetFramework>netstandard2.1</TargetFramework>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>email, parameter dictionary, random, core</PackageTags>
    <IsPackable>true</IsPackable>
    <ProjectURL>https://github.com/Burkhardt/RaiUtils</ProjectURL>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Copyright>Â© Rainer Burkhardt. All rights reserved.</Copyright>
    <RepositoryUrl>https://github.com/Burkhardt/RaiUtils</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageReleaseNotes>Version for netstandard2.1</PackageReleaseNotes>
    <!--<PackageIcon></PackageIcon>-->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.json" Version="13.0.1" />
  </ItemGroup>

</Project>
